	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/bbc-lc1.0.0/ && (cd `dirname .tmp/bbc-lc1.0.0` && $(TAR) bbc-lc1.0.0.tar bbc-lc1.0.0 && $(COMPRESS) bbc-lc1.0.0.tar) && $(MOVE) `dirname .tmp/bbc-lc1.0.0`/bbc-lc1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/bbc-lc1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


check: first

compiler_yacc_decl_make_all:
compiler_yacc_decl_clean:
compiler_yacc_impl_make_all:
compiler_yacc_impl_clean:
compiler_lex_make_all:
compiler_lex_clean:
compiler_clean: 

####### Compile

main.o: ../main.cpp ../commentarymodel.h \
		../competition.h \
		../game.h \
		../iobserver.h \
		../cursesview.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o ../main.cpp

competition.o: ../competition.cpp ../competition.h \
		../game.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o competition.o ../competition.cpp

game.o: ../game.cpp ../game.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o game.o ../game.cpp

commentarymodel.o: ../commentarymodel.cpp ../commentarymodel.h \
		../competition.h \
		../game.h \
		../iobserver.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o commentarymodel.o ../commentarymodel.cpp

cursesview.o: ../cursesview.cpp ../cursesview.h \
		../commentarymodel.h \
		../competition.h \
		../game.h \
		../iobserver.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o cursesview.o ../cursesview.cpp

####### Install

install:   FORCE

uninstall:   FORCE

